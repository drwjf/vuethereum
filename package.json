{
  "name": "vuethereum",
  "version": "0.0.4",
  "description": "The Vue library Ethereum deserves",
  "main": "dist/vuethereum.umd.js",
  "module": "dist/vuethereum.esm.js",
  "unpkg": "dist/vuethereum.min.js",
  "files": [
    "/src",
    "/dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/matryx/vuethereum.git"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "echo TODO && exit 1",
    "build": "npm run build:umd & npm run build:es & npm run build:unpkg",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/vuethereum.umd.js",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist/vuethereum.esm.js",
    "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/vuethereum.min.js"
  },
  "keywords": [
    "vue",
    "ethereum",
    "web3",
    "smart",
    "contract",
    "metamask"
  ],
  "author": "Matryx by Nanome <dev@nanome.ai>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/matryx/vuethereum/issues"
  },
  "homepage": "https://github.com/matryx/vuethereum#readme",
  "dependencies": {
    "blockies": "0.0.2",
    "ethers": "^3.0.26"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@vue/test-utils": "^1.0.0-beta.28",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.5",
    "babel-preset-vue": "^2.0.2",
    "jest": "^23.6.0",
    "minimist": "^1.2.0",
    "rollup": "^1.1.0",
    "rollup-plugin-buble": "^0.19.6",
    "rollup-plugin-filesize": "^6.0.0",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-uglify-es": "0.0.1",
    "rollup-plugin-vue": "^4.3.2",
    "stylelint": "^9.9.0",
    "stylelint-config-sass-guidelines": "^5.3.0",
    "vue": "^2.5.22",
    "vue-loader": "^15.5.1",
    "vue-template-compiler": "^2.5.22"
  }
}
